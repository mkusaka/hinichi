    "hono": "^4.11.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.24.0",
    "@cloudflare/vitest-pool-workers": "0.12.11",
    "@cloudflare/workers-types": "4.20260212.0",
    "@vitest/runner": "3.2.4",
    "@vitest/snapshot": "3.2.4",
    "oxfmt": "0.31.0",
    "oxlint": "1.46.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "3.2.4",
    "wrangler": "4.64.0"
  }
